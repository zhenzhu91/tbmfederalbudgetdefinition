            <choice>
                <element inactive_on_update="false" label="Draft" sequence="1" value="draft"/>
                <element inactive_on_update="false" label="Submitted" sequence="2" value="submitted"/>
                <element inactive_on_update="false" label="Reviewed" sequence="3" value="reviewed"/>
                <element inactive_on_update="false" label="Rejected" sequence="4" value="rar"/>
                <element inactive_on_update="false" label="Rejected" sequence="5" value="not approved"/>
                <element inactive_on_update="false" label="Approved" sequence="6" value="approved"/>
            </choice>
        </element>
        <element default="javascript:current.getTableName();" label="Class" max_length="80" name="sys_class_name" type="sys_class_name"/>
        <element label="Watch List" max_length="4000" name="watch_list" reference="sys_user" type="glide_list"/>
        <index name="index">
            <element name="cost_sub_pool"/>
        </index>
        <index name="index2">
            <element name="fund_source"/>
        </index>
        <index name="index3">
            <element name="cost_pool"/>
        </index>
        <index name="index4">
