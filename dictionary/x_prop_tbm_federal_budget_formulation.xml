<?xml version="1.0" encoding="UTF-8"?><database>
    <element audit="true" label="Budget Formulation" max_length="40" name="x_prop_tbm_federal_budget_formulation" type="collection">
        <element label="Budget  Amount" mandatory="true" max_length="20" name="budget_amount" type="currency"/>
        <element label="Budget Line Item" mandatory="true" max_length="40" name="budget_line_item" type="string"/>
        <element label="Comments" max_length="4000" name="comments" type="journal_input"/>
        <element label="Config Org 1" max_length="40" name="config_org_1" type="string"/>
        <element label="Config Org 2" max_length="40" name="config_org_2" type="string"/>
        <element label="Config Org 3" max_length="40" name="config_org_3" type="string"/>
